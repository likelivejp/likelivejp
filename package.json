    "firebase-admin": "^8.0.0",
    "firebase-functions": "^3.0.0",
    "foreman": "^3.0.1",
    "markdown-it": "^10.0.0",
    "node-sass": "^7.0.0",
    "nuxt": "^2.10.2",
    "prettier": "^1.19.1",
    "processmd": "^4.5.0",
    "rfs": "^9.0.2",
    "sass-loader": "^8.0.0",
